{
  "name": "blasio-app",
  "version": "0.1.0-rc.1",
  "description": "Blasio video platform desktop app",
  "main": "./dist/main.prod.js",
  "scripts": {
    "clear": "rm -rf ./dist",
    "dev": "npm run start-renderer",
    "prod": "run-electron .",
    "package": "npm run build && electron-packager --electron-version=8.3.1 . --overwrite --out=build --icon=src/renderer/assets/img/app.ico",
    "build": "npm run clear && npm-run-all --parallel build-main build-renderer",
    "build-main": "webpack --config webpack/main.config.js",
    "build-renderer": "webpack --config webpack/renderer.prod.config.js",
    "start-main": "cross-env NODE_ENV=development run-electron -r @babel/register src/main/index.js",
    "start-renderer": "cross-env NODE_ENV=development webpack-dev-server --port 6789 --config webpack/renderer.dev.config.js",
    "test": "mocha --require @babel/register --slow 0"
  },
  "keywords": [
    "electron",
    "react",
    "blasio",
    "video",
    "decentralized",
    "orbitdb"
  ],
  "author": "vaultec81",
  "license": "GPL-3.0",
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.0",
    "babel-loader": "^8.0.5",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "electron": "^8.5.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "mocha": "^8.0.1",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "run-electron": "^0.1.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "tmp": "^0.2.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@hiveio/dhive": "^0.14.1",
    "@hiveio/hive-js": "^0.8.2",
    "arraysearch": "^1.2.0",
    "axios": "^0.19.2",
    "bootstrap": "^4.5.0",
    "compressjs": "^1.0.3",
    "convert-units": "^2.3.4",
    "datastore-fs": "^2.0.0",
    "datastore-level": "^1.1.0",
    "date-and-time": "^0.13.1",
    "dlv": "^1.1.3",
    "electron-packager": "^14.2.1",
    "electron-promise-ipc": "^2.1.0",
    "i18next": "^19.4.5",
    "ipld-dag-cbor": "^0.15.3",
    "jquery": "^3.5.1",
    "lokijs": "^1.5.9",
    "merge-options": "^2.0.0",
    "node-schedule": "^1.3.2",
    "p-queue": "^6.6.0",
    "pouchdb": "^7.1.1",
    "pouchdb-find": "^7.2.1",
    "query-string": "^6.13.1",
    "react": "^16.8.5",
    "react-bootstrap": "^1.0.1",
    "react-dom": "^16.8.5",
    "react-i18next": "^11.6.0",
    "react-icons": "^3.10.0",
    "react-jw-player": "^1.19.1",
    "react-markdown": "^4.3.1",
    "react-router-dom": "^5.2.0"
  }
}
